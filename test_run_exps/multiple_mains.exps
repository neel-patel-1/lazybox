# first experiment
start echo '6 and 2'
main for i in `seq 1 6`; \
     do \
        echo 'main 6: '$i; \
        sleep 1; \
     done
main for i in `seq 1 2`; \
     do \
        echo 'main 2: '$i; \
        sleep 1; \
     done
end echo 'end!'

